---
import Head, { Props as HeadProps } from '$/components/Head.astro';
import HTML from '$/components/HTML.astro';
import BaseLayout from '$/components/BaseLayout.astro';

export interface Props {
  head: HeadProps;
}

const { head } = Astro.props;
---

<HTML>
  <Head {...head} />
  <BaseLayout>
    <slot />
  </BaseLayout>
</HTML>
